{
  "manifest_version": 3,
  "name": "AutoFormFillerPoC",
  "version": "1.0",
  "description": "Barebones POC for an extension that will allow automation of form filling for various sites with user provided data.",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://www.google.pl/*"
  ],
  "action": {
    "default_title": "AutoFormFiller",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "icons/icon48.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dkrucze@gmail.com"
    }
  }
}